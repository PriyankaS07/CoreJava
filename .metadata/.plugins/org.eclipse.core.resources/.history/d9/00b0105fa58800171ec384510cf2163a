package com.spring.FirstDemo;

public class Order {
	public int getOrderNo() {
		return orderNo;
	}
	public void setOrderNo(int orderNo) {
		this.orderNo = orderNo;
	}
	public Item getItem() {
		return item;
	}
	public void setItem(Item item) {
		this.item = item;
	}
	public int orderNo;
	Item item;
	public Order(int orderNo, Item item) {
		super();
		this.orderNo = orderNo;
		this.item = item;
	}
}
